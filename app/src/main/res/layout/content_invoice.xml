<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.cardview.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            card_view:cardCornerRadius="3dp"
            card_view:cardElevation="3dp"
            card_view:cardMaxElevation="3dp"
            card_view:cardUseCompatPadding="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:orientation="vertical">

                <androidx.core.widget.NestedScrollView
                    android:id="@+id/nsv_data_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            card_view:cardCornerRadius="3dp"
                            card_view:cardElevation="3dp"
                            card_view:cardMaxElevation="3dp"
                            card_view:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:id="@+id/rl_view_customer"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:background="@drawable/view_clickable_invoice"
                                    android:paddingStart="8dp"
                                    android:paddingLeft="8dp"
                                    android:paddingEnd="8dp"
                                    android:paddingRight="8dp">

                                    <TextView
                                        style="@style/textView"
                                        android:id="@+id/rl_tab_customer"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="7dp"
                                        android:layout_marginLeft="7dp"
                                        android:background="@drawable/view_clickable_trasperant"
                                        android:drawableRight="@drawable/ic_expand"
                                        android:drawableEnd="@drawable/ic_expand"
                                        android:layout_toStartOf="@+id/iv_info"
                                        android:layout_toLeftOf="@+id/iv_info"
                                        app:fontFamily="@font/helvetica_medium"
                                        android:gravity="center|start"
                                        android:text="@string/customer_details"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="16sp" />


                                    <ImageView
                                        android:id="@+id/iv_info"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:contentDescription="@string/app_name"
                                        android:src="@drawable/ic_collapse"
                                        android:visibility="gone" />


                                </RelativeLayout>

                                <LinearLayout
                                    android:id="@+id/ll_customer_view"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginLeft="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:layout_marginRight="8dp"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/tv_user_name"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="14sp" />

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/delivery_address"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_address"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:maxLines="5"
                                        android:singleLine="false"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />
                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/landmark"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_landmark"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:maxLines="5"
                                        android:singleLine="false"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />


                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="0.2dp"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/edit_txt" />


                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/contact_number"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_contact_number"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        android:drawableStart="@drawable/ic_vec_mobile"
                                        android:drawableLeft="@drawable/ic_vec_mobile"
                                        android:drawablePadding="15dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="0.2dp"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/edit_txt" />

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/email_id"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_email"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        android:drawableStart="@drawable/ic_vec_email"
                                        android:drawableLeft="@drawable/ic_vec_email"
                                        android:drawablePadding="15dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="15dp" />


                                </LinearLayout>

                            </LinearLayout>


                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            card_view:cardCornerRadius="3dp"
                            card_view:cardElevation="3dp"
                            card_view:cardMaxElevation="3dp"
                            card_view:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">


                                <TextView
                                    style="@style/textView"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:background="@color/invoice_tab"
                                    app:fontFamily="@font/helvetica_medium"
                                    android:gravity="center|start"
                                    android:paddingStart="15dp"
                                    android:paddingLeft="15dp"
                                    android:paddingEnd="8dp"
                                    android:paddingRight="8dp"
                                    android:text="@string/order_details"
                                    android:textColor="@color/edit_txt"
                                    android:textSize="16sp" />


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginLeft="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:layout_marginRight="8dp"
                                    android:orientation="vertical">


                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/order_number"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_order_number"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:maxLines="5"
                                        android:singleLine="false"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="0.2dp"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/edit_txt" />


                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/order_date"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_order_date"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="0.2dp"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/edit_txt" />

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/payment"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_payment"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="0.2dp"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/edit_txt" />


                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/order_type"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_order_type"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />
                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="0.2dp"
                                        android:layout_marginTop="10dp"
                                        android:visibility="gone"
                                        android:background="@color/edit_txt" />
                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:layout_marginTop="10dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/payment_status"
                                        android:visibility="gone"
                                        android:textColor="@color/black"
                                        android:textSize="15sp" />
                                    <TextView
                                        android:id="@+id/tv_payment_status"
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="2dp"
                                        android:layout_marginLeft="2dp"
                                        android:visibility="gone"
                                        android:layout_marginTop="2dp"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="13sp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="15dp" />


                                </LinearLayout>

                            </LinearLayout>


                        </androidx.cardview.widget.CardView>


                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recycler_view"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:scrollbars="none" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="20dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/item_prize_top"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:paddingStart="10dp"
                                    android:paddingLeft="10dp"
                                    android:paddingEnd="10dp"
                                    android:paddingRight="10dp">

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_centerVertical="true"
                                        android:layout_toStartOf="@+id/tv_sub_total"
                                        android:layout_toLeftOf="@+id/tv_sub_total"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/sub_total"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="16sp" />

                                    <TextView
                                        android:id="@+id/tv_sub_total"
                                        style="@style/textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        app:fontFamily="@font/helvetica_medium"
                                        android:gravity="center|start"
                                        android:textColor="@color/black"
                                        android:textSize="16sp" />

                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:paddingStart="10dp"
                                    android:paddingLeft="10dp"
                                    android:paddingEnd="10dp"
                                    android:visibility="gone"
                                    android:paddingRight="10dp">

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_centerVertical="true"
                                        android:layout_toStartOf="@+id/tv_total_tax"
                                        android:layout_toLeftOf="@+id/tv_total_tax"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/tax_small"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="16sp" />

                                    <TextView
                                        android:id="@+id/tv_total_tax"
                                        style="@style/textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        app:fontFamily="@font/helvetica_medium"
                                        android:gravity="center|start"
                                        android:textColor="@color/black"
                                        android:textSize="16sp" />

                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/item_prize_top"
                                    android:paddingStart="10dp"
                                    android:paddingLeft="10dp"
                                    android:paddingEnd="10dp"
                                    android:paddingRight="10dp">


                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_centerVertical="true"
                                        android:layout_toStartOf="@+id/tv_delivery_fee"
                                        android:layout_toLeftOf="@+id/tv_delivery_fee" >

                                        <TextView
                                            style="@style/textView"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            app:fontFamily="@font/helvetica_light"
                                            android:gravity="center|start"
                                            android:text="@string/delivery_fees"
                                            android:textColor="@color/edit_txt"
                                            android:textSize="16sp" />

                                        <ImageView
                                            android:visibility="gone"
                                            android:background="?selectableItemBackground"
                                            android:id="@+id/ivPeakHours"
                                            android:layout_marginTop="-10dp"
                                            android:padding="10dp"
                                            android:layout_width="wrap_content"
                                            android:src="@drawable/ic_peak_hours"
                                            android:layout_height="wrap_content" />


                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/tv_delivery_fee"
                                        style="@style/textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        app:fontFamily="@font/helvetica_medium"
                                        android:gravity="center|start"
                                        android:textColor="@color/black"
                                        android:textSize="16sp" />

                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rl_use_wallet"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:paddingStart="10dp"
                                    android:paddingLeft="10dp"
                                    android:paddingEnd="10dp"
                                    android:paddingRight="10dp"
                                    android:visibility="gone">

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_centerVertical="true"
                                        android:layout_toStartOf="@+id/tv_use_wallet"
                                        android:layout_toLeftOf="@+id/tv_use_wallet"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/use_wallet"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="16sp" />

                                    <TextView
                                        android:id="@+id/tv_use_wallet"
                                        style="@style/textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        app:fontFamily="@font/helvetica_medium"
                                        android:gravity="center|start"
                                        android:textColor="@color/black"
                                        android:textSize="16sp" />

                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/rl_use_offer"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:paddingStart="10dp"
                                    android:paddingLeft="10dp"
                                    android:paddingEnd="10dp"
                                    android:paddingRight="10dp"
                                    android:visibility="gone">

                                    <TextView
                                        style="@style/textView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentStart="true"
                                        android:layout_alignParentLeft="true"
                                        android:layout_centerVertical="true"
                                        android:layout_toStartOf="@+id/tv_use_offer"
                                        android:layout_toLeftOf="@+id/tv_use_offer"
                                        app:fontFamily="@font/helvetica_light"
                                        android:gravity="center|start"
                                        android:text="@string/use_coupon"
                                        android:textColor="@color/edit_txt"
                                        android:textSize="16sp" />

                                    <TextView
                                        android:id="@+id/tv_use_offer"
                                        style="@style/textView"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentEnd="true"
