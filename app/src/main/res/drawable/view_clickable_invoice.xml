<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/color_orange" />
            <gradient android:angle="-90" android:endColor="@color/color_orange" android:startColor="@color/color_orange" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/invoice_tab" />
            <gradient android:angle="-90" android:endColor="@color/invoice_tab" android:startColor="@color/invoice_tab" />
        </shape>
    </item>


</selector>