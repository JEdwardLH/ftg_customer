<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="23dip" />
            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
            <stroke android:width="1dip" android:color="#3A5896" />
            <gradient android:angle="-90" android:endColor="#3A5896" android:startColor="#3A5896" />
        </shape>

    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="23dip" />

            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
            <stroke android:width="1dip" android:color="#FFFFFF" />
            <gradient android:angle="-90" android:endColor="#FFFFFF" android:startColor="#FFFFFF" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="23dip" />

            <padding android:bottom="5dp" android:left="5dp" android:right="5dp" android:top="5dp" />
            <stroke android:width="1dip" android:color="#FFFFFF" />
            <gradient android:angle="-90" android:endColor="#FFFFFF" android:startColor="#FFFFFF" />
        </shape>
    </item>
</selector>
